#include <iostream>
using namespace std;
int main(){char ch;
    cout<<"Enter a Character:";
    cin>>ch;
    if(ch=='a'||ch=='e'||ch=='i'||ch=='o'||ch=='u'){
        cout<<ch<<" is Vowel"<<endl;
    }else{
        cout<<ch<<" is Constant"<<endl;}
    return 0;}
/*Output:-
Enter a Character:b
b is Constant
*/
